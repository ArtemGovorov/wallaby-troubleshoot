    "webpack-dev-server": "^2.11.1"
  },
  "config": {
    "tsfiles": "{app,__{tests,mocks}__}/**/*.{ts,tsx}",
    "jsfiles": "{app,__{tests,mocks}__}/**/*.{js,jsx}",
    "prettierpath": "{,!(node_modules)/**/}*.{js,jsx,json,babelrc,ts,tsx}"
  },
  "scripts": {
    "start": "./bin/webpack-dev-server",
    "build": "./bin/webpack",
    "test": "jest --maxWorkers=4 --verbose --coverage --mapCoverage",
    "test:changed": "jest -o --watch --maxWorkers=4",
    "lint": "tslint --project tsconfig.json $npm_package_config_tsfiles && eslint $npm_package_config_jsfiles",
    "lint:fix": "tslint --project tsconfig.json $npm_package_config_tsfiles --fix && eslint $npm_package_config_jsfiles --fix",
    "precommit": "lint-staged && tsc --noEmit -p tsconfig.json",
    "release": "node ./scripts/release.js",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "reverse-proxy": "node ./scripts/reverse-proxy-80-to-3000.js &"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "useBabelrc": true
      }
    },
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/app/javascript"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setupTest.js",
    "setupFiles": [
      "<rootDir>/config/jest/testShim.js",
      "<rootDir>/config/jest/testSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "roots": [
      "<rootDir>/app/javascript/easybib"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "testRegex": "((\\.|/)test)\\.(js|jsx|ts|tsx)$",
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  }
}
